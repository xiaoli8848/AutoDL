<ResourceDictionary
    x:Class="AutoDL.AutoDLResource"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="using:AutoDL"
    mc:Ignorable="d">

    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush Color="LightGray" x:Key="SettingItemBorderBrush" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <SolidColorBrush Color="Gray" x:Key="SettingItemBorderBrush" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
    <local:InverseBooleanConverter x:Key="InverseBooleanConverter" />
    <XamlUICommand x:Key="AddImageWallpaper" ExecuteRequested="AddImageWallpaper_OnExecuteRequested" />
    <XamlUICommand x:Key="AddWallpaperEngineWallpaper"
                   ExecuteRequested="AddWallpaperEngineWallpaper_OnExecuteRequested" />

</ResourceDictionary>